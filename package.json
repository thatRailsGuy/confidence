{
  "name": "confidence-eleventy",
  "version": "1.0.0",
  "description": "NFL Confidence Pool static site built with Eleventy",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "eleventy --config=.eleventy.cjs",
    "serve": "eleventy --serve --config=.eleventy.cjs",
    "debug": "DEBUG=Eleventy* eleventy --config=.eleventy.cjs",
    "fetch-odds": "node scripts/fetch-odds.js",
    "clean": "rm -rf _site",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e"
  },
  "keywords": [
    "eleventy",
    "nfl",
    "confidence",
    "static-site"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@playwright/test": "^1.40.0",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.0.1"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  }
}
